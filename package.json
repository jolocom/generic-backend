{
  "name": "Generic-backend",
  "version": "0.4.0",
  "private": false,
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "clean": "rm -rf ./build",
    "start": "yarn run redis:start; ts-node-dev --respawn --transpileOnly src/server.ts; yarn run redis:stop",
    "server": "node ./build/server.js",
    "redis:start": "redis-server &",
    "redis:stop": "redis-cli SHUTDOWN",
    "register": "node ./bin/register.js"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "cred-types-jolocom-core": "^0.0.10",
    "cred-types-jolocom-demo": "^0.2.1",
    "email-validator": "^2.0.4",
    "express": "^4.17.3",
    "jolocom-lib": "^4.1.0",
    "redis": "^2.8.0",
    "socket.io": "^2.2.0",
    "tslib": "^1.9.3",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.0.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/node": "^11.9.5",
    "@types/redis": "^2.8.6",
    "@types/socket.io": "^2.1.2",
    "prettier": "^1.15.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.0.1"
  }
}
